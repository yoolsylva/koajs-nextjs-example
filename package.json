{
  "name": "koa-invoice-api",
  "description": "Koa 2 RESTful API boilerplate",
  "version": "1.0.0",
  "author": "Sylva",
  "keywords": [
    "koa",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "async",
    "es7"
  ],
  "license": "MIT",
  "main": "api_server/index.js",
  "scripts": {
    "start": "nodemon api_server/ --exec babel-node & cd next_server/ && node server.js",
    "build": "babel api_server -d dist",
    "prod": "yarn run build && node dist/ & cd next_server/ && node server.js"
  },
  "dependencies": {
    "@koa/cors": "2",
    "babel-polyfill": "^6.5.0",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "jsonwebtoken": "^8.1.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-helmet": "^3.3.0",
    "koa-jwt": "^3.3.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "mongoose": "^5.0.1",
    "next": "^5.1.0",
    "node-fetch": "^2.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.8.1",
    "should": "^13.1.3",
    "supertest": "^3.0.0"
  }
}
